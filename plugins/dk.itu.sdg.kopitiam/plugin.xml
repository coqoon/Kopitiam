<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="Kopitiam.step_forward"
            contextId="Kopitiam.context"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="Ctrl+N">
      </key>
      <key
            commandId="Kopitiam.step_cursor"
            contextId="Kopitiam.context"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="Ctrl+Enter">
      </key>
      <key
            commandId="Kopitiam.step_backward"
            contextId="Kopitiam.context"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="Ctrl+U">
      </key>
      <key
            commandId="Kopitiam.retract"
            contextId="Kopitiam.context"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="Ctrl+R">
      </key>
      <key
            commandId="Kopitiam.step_all"
            contextId="Kopitiam.context"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="Ctrl+B">
      </key>
      <key
            commandId="Kopitiam.interrupt"
            contextId="Kopitiam.context"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="Ctrl+C">
      </key>
      <key
            commandId="Kopitiam.toggle_coq_flag"
            contextId="Kopitiam.context"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+M3+I">
         <parameter
               id="Kopitiam.toggle_coq_flag.name"
               value="Printing Implicit">
         </parameter>
      </key>
      <key
            commandId="Kopitiam.toggle_coq_flag"
            contextId="Kopitiam.context"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+M3+C">
         <parameter
               id="Kopitiam.toggle_coq_flag.name"
               value="Printing Coercions">
         </parameter>
      </key>
      <key
            commandId="Kopitiam.toggle_coq_flag"
            contextId="Kopitiam.context"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+M3+M">
         <parameter
               id="Kopitiam.toggle_coq_flag.name"
               value="Printing Matching">
         </parameter>
      </key>
      <key
            commandId="Kopitiam.toggle_coq_flag"
            contextId="Kopitiam.context"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+M3+S">
         <parameter
               id="Kopitiam.toggle_coq_flag.name"
               value="Printing Synth">
         </parameter>
      </key>
      <key
            commandId="Kopitiam.toggle_coq_flag"
            contextId="Kopitiam.context"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+M3+N">
         <parameter
               id="Kopitiam.toggle_coq_flag.name"
               value="Printing Notations">
         </parameter>
      </key>
      <key
            commandId="Kopitiam.toggle_coq_flag"
            contextId="Kopitiam.context"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+M3+A">
         <parameter
               id="Kopitiam.toggle_coq_flag.name"
               value="Printing All">
         </parameter>
      </key>
      <key
            commandId="Kopitiam.toggle_coq_flag"
            contextId="Kopitiam.context"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+M3+R">
         <parameter
               id="Kopitiam.toggle_coq_flag.name"
               value="Printing Records">
         </parameter>
      </key>
      <key
            commandId="Kopitiam.toggle_coq_flag"
            contextId="Kopitiam.context"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+M3+E">
         <parameter
               id="Kopitiam.toggle_coq_flag.name"
               value="Printing Existential Instances">
         </parameter>
      </key>
      <key
            commandId="Kopitiam.toggle_coq_flag"
            contextId="Kopitiam.context"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+M3+U">
         <parameter
               id="Kopitiam.toggle_coq_flag.name"
               value="Printing Universes">
         </parameter>
      </key>
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="dk.itu.sdg.kopitiam.CoqEditor"
            contributorClass="org.eclipse.ui.texteditor.BasicTextEditorActionContributor"
            extensions="v"
            icon="icons/coqfile_obj.gif"
            id="kopitiam.CoqEditor"
            name="Coq Editor">
         <contentTypeBinding
               contentTypeId="dk.itu.sdg.kopitiam.CoqFile">
         </contentTypeBinding>
      </editor>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <category
            id="Kopitiam"
            name="Coq Views">
      </category>
      <view
            allowMultiple="false"
            category="Kopitiam"
            class="dk.itu.sdg.kopitiam.GoalViewer"
            icon="icons/coqide.png"
            id="kopitiam.GoalViewer"
            name="Goal Viewer">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="org.eclipse.jdt.ui.JavaPerspective">
         <view
               id="kopitiam.GoalViewer"
               ratio="0.5"
               relationship="right"
               relative="org.eclipse.ui.views.ContentOutline">
         </view>
      </perspectiveExtension>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            class="dk.itu.sdg.kopitiam.KopitiamPreferencePage"
            id="Kopitiam.settings"
            name="Kopitiam">
      </page>
      <page
            category="Kopitiam.settings"
            class="dk.itu.sdg.kopitiam.KopitiamColorPreferencePage"
            id="Kopitiam.colorSettings"
            name="Color Settings">
      </page>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <category
            id="Kopitiam.category"
            name="Kopitiam">
      </category>
      <command
            categoryId="Kopitiam.category"
            id="Kopitiam.step_forward"
            name="Step forward command">
      </command>
      <command
            categoryId="Kopitiam.category"
            id="Kopitiam.step_backward"
            name="Step backward command">
      </command>
      <command
            categoryId="Kopitiam.category"
            id="Kopitiam.step_cursor"
            name="Step to cursor command">
      </command>
      <command
            categoryId="Kopitiam.category"
            id="Kopitiam.step_all"
            name="Step all command">
      </command>
      <command
            categoryId="Kopitiam.category"
            id="Kopitiam.retract"
            name="Retract all command">
      </command>
      <command
            categoryId="Kopitiam.category"
            id="Kopitiam.stop_coq"
            name="Stop Coq command">
      </command>
      <command
            categoryId="Kopitiam.category"
            id="Kopitiam.interrupt"
            name="Interrupt Coq command">
      </command>
      <command
            categoryId="Kopitiam.category"
            id="Kopitiam.verify_method"
            name="Verify Java method command">
      </command>
      <command
            categoryId="Kopitiam.category"
            id="Kopitiam.save_proof_certificate"
            name="Save Java proof certificate command">
      </command>
      <command
            categoryId="Kopitiam.category"
            description="Toggle a Coq flag"
            id="Kopitiam.toggle_coq_flag"
            name="Toggle Coq flag command">
         <commandParameter
               id="Kopitiam.toggle_coq_flag.name"
               name="Flag"
               optional="false">
         </commandParameter>
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.contexts">
      <context
            description="Context for Kopitiam files"
            id="Kopitiam.context"
            name="In Kopitiam"
            parentId="org.eclipse.ui.textEditorScope">
      </context>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="toolbar:org.eclipse.ui.main.toolbar">
         <toolbar
               id="Kopitiam.coqToolbar"
               label="Kopitiam toolbar">
            <command
                  commandId="Kopitiam.step_backward"
                  icon="icons/up.gif"
                  label="Undo"
                  style="push"
                  tooltip="Undo">
            </command>
            <command
                  commandId="Kopitiam.retract"
                  icon="icons/uptoo.gif"
                  label="Retract"
                  style="push"
                  tooltip="Retract">
            </command>
            <command
                  commandId="Kopitiam.step_forward"
                  icon="icons/down.gif"
                  label="Step"
                  style="push"
                  tooltip="Step">
            </command>
            <command
                  commandId="Kopitiam.step_cursor"
                  icon="icons/icon_lego.gif"
                  label="Step Until Cursor"
                  style="push"
                  tooltip="Step Until Cursor">
            </command>
            <command
                  commandId="Kopitiam.step_all"
                  icon="icons/downtoo.gif"
                  label="Step All"
                  style="push"
                  tooltip="Step All">
            </command>
            <command
                  commandId="Kopitiam.stop_coq"
                  icon="icons/dead.gif"
                  label="Stop Coq"
                  style="push"
                  tooltip="Stop Coq">
            </command>
            <command
                  commandId="Kopitiam.interrupt"
                  icon="icons/stop.gif"
                  label="Interrupt Coq"
                  style="push"
                  tooltip="Interrupt Coq">
            </command>
         </toolbar>
      </menuContribution>
      <menuContribution
            allPopups="true"
            locationURI="popup:org.eclipse.jdt.ui.CompilationUnitEditor.EditorContext">
         <command
               commandId="Kopitiam.verify_method"
               icon="icons/icon_lego.gif"
               id="dk.itu.sdg.kopitiam.popupContribution"
               label="Verify method"
               style="push"
               tooltip="Verify method">
         </command>
         <command
               commandId="Kopitiam.save_proof_certificate"
               icon="icons/checkmark.gif"
               label="Save proof certificate"
               style="push"
               tooltip="Save proof certificate">
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="menu:org.eclipse.ui.main.menu">
         <menu
               id="Kopitiam.coqMenu"
               label="Coq">
            <command
                  commandId="Kopitiam.step_backward"
                  icon="icons/up.gif"
                  label="Undo"
                  style="push"
                  tooltip="Undo">
            </command>
            <command
                  commandId="Kopitiam.retract"
                  icon="icons/uptoo.gif"
                  label="Retract"
                  style="push"
                  tooltip="Retract">
            </command>
            <command
                  commandId="Kopitiam.step_forward"
                  icon="icons/down.gif"
                  label="Step"
                  style="push"
                  tooltip="Step">
            </command>
            <command
                  commandId="Kopitiam.step_cursor"
                  icon="icons/icon_lego.gif"
                  label="Step Until Cursor"
                  style="push"
                  tooltip="Step Until Cursor">
            </command>
            <command
                  commandId="Kopitiam.step_all"
                  icon="icons/downtoo.gif"
                  label="Step All"
                  style="push"
                  tooltip="Step All">
            </command>
            <command
                  commandId="Kopitiam.stop_coq"
                  icon="icons/dead.gif"
                  label="Stop Coq"
                  style="push"
                  tooltip="Stop Coq">
            </command>
            <command
                  commandId="Kopitiam.interrupt"
                  icon="icons/stop.gif"
                  label="Interrupt Coq"
                  style="push"
                  tooltip="Interrupt Coq">
            </command>
            <separator
                  name="Kopitiam.coqMenu.separator1"
                  visible="true">
            </separator>
            <menu
                  id="Kopitiam.coqMenu.setPrinting"
                  label="Set Printing">
               <command
                     commandId="Kopitiam.toggle_coq_flag"
                     label="Implicit"
                     style="toggle">
                  <parameter
                        name="Kopitiam.toggle_coq_flag.name"
                        value="Printing Implicit">
                  </parameter>
               </command>
               <command
                     commandId="Kopitiam.toggle_coq_flag"
                     label="Coercions"
                     style="toggle">
                  <parameter
                        name="Kopitiam.toggle_coq_flag.name"
                        value="Printing Coercions">
                  </parameter>
               </command>
               <command
                     commandId="Kopitiam.toggle_coq_flag"
                     label="Matching"
                     style="toggle">
                  <parameter
                        name="Kopitiam.toggle_coq_flag.name"
                        value="Printing Matching">
                  </parameter>
               </command>
               <command
                     commandId="Kopitiam.toggle_coq_flag"
                     label="Synth"
                     style="toggle">
                  <parameter
                        name="Kopitiam.toggle_coq_flag.name"
                        value="Printing Synth">
                  </parameter>
               </command>
               <command
                     commandId="Kopitiam.toggle_coq_flag"
                     label="Notations"
                     style="toggle">
                  <parameter
                        name="Kopitiam.toggle_coq_flag.name"
                        value="Printing Notations">
                  </parameter>
               </command>
               <command
                     commandId="Kopitiam.toggle_coq_flag"
                     label="All"
                     style="toggle">
                  <parameter
                        name="Kopitiam.toggle_coq_flag.name"
                        value="Printing All">
                  </parameter>
               </command>
               <command
                     commandId="Kopitiam.toggle_coq_flag"
                     label="Records"
                     style="toggle">
                  <parameter
                        name="Kopitiam.toggle_coq_flag.name"
                        value="Printing Records">
                  </parameter>
               </command>
               <command
                     commandId="Kopitiam.toggle_coq_flag"
                     label="Existential Instances"
                     style="toggle">
                  <parameter
                        name="Kopitiam.toggle_coq_flag.name"
                        value="Printing Existential Instances">
                  </parameter>
               </command>
               <command
                     commandId="Kopitiam.toggle_coq_flag"
                     label="Universes"
                     style="toggle">
                  <parameter
                        name="Kopitiam.toggle_coq_flag.name"
                        value="Printing Universes">
                  </parameter>
               </command>
            </menu>
            <separator
                  name="Kopitiam.coqMenu.separator2"
                  visible="true">
            </separator>
            <menu
                  id="Kopitiam.coqMenu.javaMenu"
                  label="Java">
               <command
                     commandId="Kopitiam.verify_method"
                     icon="icons/icon_lego.gif"
                     id="dk.itu.sdg.kopitiam.popupContribution"
                     label="Verify method"
                     style="push"
                     tooltip="Verify method">
               </command>
               <command
                     commandId="Kopitiam.save_proof_certificate"
                     icon="icons/checkmark.gif"
                     label="Save proof certificate"
                     style="push"
                     tooltip="Save proof certificate">
               </command>
            </menu>
            <visibleWhen
                  checkEnabled="false">
               <with
                     variable="activeEditor">
                  <adapt
                        type="dk.itu.sdg.kopitiam.CoqTopContainer">
                  </adapt>
               </with>
            </visibleWhen>
         </menu>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.newWizards">
      <wizard
            category="Kopitiam.coq"
            class="dk.itu.sdg.kopitiam.NewCoqFileWizard"
            icon="icons/coqide.png"
            id="Kopitiam.newCoqFileWizard"
            name="Coq File">
      </wizard>
      <wizard
            category="Kopitiam.coq"
            class="dk.itu.sdg.kopitiam.NewCoqProjectWizard"
            finalPerspective="dk.itu.sdg.kopitiam.CoqPerspective"
            icon="icons/coqide.png"
            id="Kopitiam.newCoqProjectWizard"
            name="Coq Project"
            preferredPerspectives="dk.itu.sdg.kopitiam.CoqPerspective"
            project="true">
      </wizard>
      <category
            id="Kopitiam.coq"
            name="Coq">
      </category>
   </extension>

   <extension
         point="org.eclipse.ui.editors.annotationTypes">
      <type
            name="dk.itu.sdg.kopitiam.processed">
      </type>
      <type
            name="dk.itu.sdg.kopitiam.processing">
      </type>
      <type
            markerSeverity="2"
            markerType="dk.itu.sdg.kopitiam.provenmarker"
            name="dk.itu.sdg.kopitiam.provenannotation"
            super="org.eclipse.ui.workbench.texteditor.warning">
      </type>
   </extension>
   <extension
         id="dk.itu.sdg.kopitiam.provenmarker"
         point="org.eclipse.core.resources.markers">
      <persistent
            value="true">
      </persistent>
      <super
            type="org.eclipse.core.resources.textmarker">
      </super>
   </extension>
   <extension
         point="org.eclipse.ui.editors.markerAnnotationSpecification">
      <specification
            annotationType="dk.itu.sdg.kopitiam.provenannotation"
            colorPreferenceKey="dk.itu.sdg.kopitiam.proverannotation.mycolorhighlightkey"
            colorPreferenceValue="50,249,255"
            highlightPreferenceKey="dk.itu.sdg.kopitiam.proverannotation.mycolorbackgroundhighlightkey"
            highlightPreferenceValue="true"
            icon="icons/checkmark.gif"
            includeOnPreferencePage="true"
            label="Coq Proven Method"
            overviewRulerPreferenceKey="dk.itu.sdg.kopitiam.proverannotation.overviewrulerkey"
            overviewRulerPreferenceValue="true"
            textPreferenceKey="provenmarkerIndicationInText"
            textPreferenceValue="true"
            textStylePreferenceKey="dk.itu.sdg.kopitiam.proverannotation.textStyle"
            textStylePreferenceValue="BOX"
            verticalRulerPreferenceKey="provenmarkerIndicationInVerticalRuler"
            verticalRulerPreferenceValue="true">
      </specification>
      <specification
            annotationType="dk.itu.sdg.kopitiam.processed"
            colorPreferenceKey="coqSentBg"
            colorPreferenceValue="118,255,133"
            highlightPreferenceKey="dk.itu.sdg.kopitiam.processed.mycolorbackgroundhighlightkey"
            highlightPreferenceValue="true"
            includeOnPreferencePage="true"
            label="Coq Processed"
            overviewRulerPreferenceKey="dk.itu.sdg.kopitiam.processed.overviewrulerkey"
            overviewRulerPreferenceValue="true"
            textPreferenceKey="processedIndicationInText"
            textPreferenceValue="true"
            textStylePreferenceKey="dk.itu.sdg.kopitiam.processed.textStyle"
            textStylePreferenceValue="BOX"
            verticalRulerPreferenceKey="processedIndicationInVerticalRuler"
            verticalRulerPreferenceValue="true">
      </specification>
      <specification
            annotationType="dk.itu.sdg.kopitiam.processing"
            colorPreferenceKey="coqSentProcessBg"
            colorPreferenceValue="255,241,117"
            highlightPreferenceKey="dk.itu.sdg.kopitiam.processing.mycolorbackgroundhighlightkey"
            highlightPreferenceValue="true"
            includeOnPreferencePage="true"
            label="Coq Processing"
            overviewRulerPreferenceKey="dk.itu.sdg.kopitiam.processing.overviewrulerkey"
            overviewRulerPreferenceValue="true"
            textPreferenceKey="processingIndicationInText"
            textPreferenceValue="true"
            textStylePreferenceKey="dk.itu.sdg.kopitiam.processing.textStyle"
            textStylePreferenceValue="BOX"
            verticalRulerPreferenceKey="processingIndicationInVerticalRuler"
            verticalRulerPreferenceValue="true">
      </specification>
   </extension>
   <extension
         point="org.eclipse.core.contenttype.contentTypes">
      <content-type
            base-type="org.eclipse.core.runtime.text"
            default-charset="UTF-8"
            file-extensions="v"
            id="dk.itu.sdg.kopitiam.CoqFile"
            name="Coq file"
            priority="normal">
      </content-type>
      <content-type
            file-extensions="vo"
            id="dk.itu.sdg.kopitiam.CoqObjectFile"
            name="Coq object file"
            priority="normal">
      </content-type>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="dk.itu.sdg.kopitiam.CoqPerspectiveFactory"
            icon="icons/coqide.png"
            id="dk.itu.sdg.kopitiam.CoqPerspective"
            name="Coq">
      </perspective>
   </extension>
   <extension
         id="dk.itu.sdg.kopitiam.CoqBuilder"
         name="Coq Builder"
         point="org.eclipse.core.resources.builders">
      <builder
            hasNature="true">
         <run
               class="dk.itu.ecloq.core.project.CoqBuilder">
         </run>
      </builder>
   </extension>
   <extension
         id="dk.itu.sdg.kopitiam.CoqNature"
         name="Coq Project Nature"
         point="org.eclipse.core.resources.natures">
      <runtime>
         <run
               class="dk.itu.ecloq.core.project.CoqNature">
         </run>
      </runtime>
      <builder
            id="dk.itu.sdg.kopitiam.CoqBuilder">
      </builder>
   </extension>
   <extension
         point="org.eclipse.ui.propertyPages">
      <page
            class="dk.itu.sdg.kopitiam.LoadPathConfigurationPage"
            icon="icons/coqide.png"
            id="dk.itu.sdg.kopitiam.propertyPages.CoqLoadPath"
            name="Coq Load Path">
         <enabledWhen>
            <adapt
                  type="org.eclipse.core.resources.IProject">
               <test
                     property="org.eclipse.core.resources.projectNature"
                     value="dk.itu.sdg.kopitiam.CoqNature">
               </test>
            </adapt>
         </enabledWhen>
      </page>
   </extension>
   <extension
         point="org.eclipse.core.runtime.adapters">
      <factory
            adaptableType="org.eclipse.jdt.internal.ui.javaeditor.JavaEditor"
            class="dk.itu.sdg.kopitiam.javap.JavaEditorStateFactory">
         <adapter
               type="dk.itu.sdg.kopitiam.CoqTopContainer">
         </adapter>
      </factory>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="dk.itu.sdg.kopitiam.CoqStepForwardHandler"
            commandId="Kopitiam.step_forward">
         <activeWhen>
            <reference
                  definitionId="Kopitiam.activeEditorIsCoq">
            </reference>
         </activeWhen>
      </handler>
      <handler
            class="dk.itu.sdg.kopitiam.CoqStepBackHandler"
            commandId="Kopitiam.step_backward">
         <activeWhen>
            <reference
                  definitionId="Kopitiam.activeEditorIsCoq">
            </reference>
         </activeWhen>
      </handler>
      <handler
            class="dk.itu.sdg.kopitiam.CoqStepToCursorHandler"
            commandId="Kopitiam.step_cursor">
         <activeWhen>
            <reference
                  definitionId="Kopitiam.activeEditorIsCoq">
            </reference>
         </activeWhen>
      </handler>
      <handler
            class="dk.itu.sdg.kopitiam.CoqStepAllHandler"
            commandId="Kopitiam.step_all">
         <activeWhen>
            <reference
                  definitionId="Kopitiam.activeEditorIsCoq">
            </reference>
         </activeWhen>
      </handler>
      <handler
            class="dk.itu.sdg.kopitiam.CoqRetractAllHandler"
            commandId="Kopitiam.retract">
         <activeWhen>
            <reference
                  definitionId="Kopitiam.activeEditorIsCoq">
            </reference>
         </activeWhen>
      </handler>
      <handler
            class="dk.itu.sdg.kopitiam.StopCoqHandler"
            commandId="Kopitiam.stop_coq">
         <activeWhen>
            <reference
                  definitionId="Kopitiam.activeEditorIsCoq">
            </reference>
         </activeWhen>
      </handler>
      <handler
            class="dk.itu.sdg.kopitiam.InterruptCoqHandler"
            commandId="Kopitiam.interrupt">
         <activeWhen>
            <reference
                  definitionId="Kopitiam.activeEditorIsCoq">
            </reference>
         </activeWhen>
      </handler>
      <handler
            class="dk.itu.sdg.kopitiam.javap.VerifyMethodHandler"
            commandId="Kopitiam.verify_method">
         <activeWhen>
            <reference
                  definitionId="Kopitiam.activeEditorIsJava">
            </reference>
         </activeWhen>
      </handler>
      <handler
            class="dk.itu.sdg.kopitiam.javap.SaveProofCertificateHandler"
            commandId="Kopitiam.save_proof_certificate">
         <activeWhen>
            <reference
                  definitionId="Kopitiam.activeEditorIsJava">
            </reference>
         </activeWhen>
      </handler>
      <handler
            class="dk.itu.sdg.kopitiam.ToggleCoqFlagHandler"
            commandId="Kopitiam.toggle_coq_flag">
         <activeWhen>
            <reference
                  definitionId="Kopitiam.activeEditorHasCoq">
            </reference>
         </activeWhen>
      </handler>
   </extension>
   <extension
         point="org.eclipse.core.expressions.definitions">
      <definition
            id="Kopitiam.activeEditorIsCoq">
         <with
               variable="activeEditor">
            <adapt
                  type="dk.itu.sdg.kopitiam.CoqTopContainer">
               <instanceof
                     value="dk.itu.sdg.kopitiam.CoqEditor">
               </instanceof>
            </adapt>
         </with>
      </definition>
      <definition
            id="Kopitiam.activeEditorIsJava">
         <with
               variable="activeEditor">
            <adapt
                  type="dk.itu.sdg.kopitiam.CoqTopContainer">
               <instanceof
                     value="dk.itu.sdg.kopitiam.javap.JavaEditorState">
               </instanceof>
            </adapt>
         </with>
      </definition>
      <definition
            id="Kopitiam.activeEditorHasCoq">
         <with
               variable="activeEditor">
            <adapt
                  type="dk.itu.sdg.kopitiam.CoqTopContainer">
            </adapt>
         </with>
      </definition>
   </extension>
   <extension
         id="dk.itu.sdg.kopitiam.problemmarker"
         name="Kopitiam problem"
         point="org.eclipse.core.resources.markers">
      <persistent
            value="true">
      </persistent>
      <super
            type="org.eclipse.core.resources.problemmarker">
      </super>
   </extension>
   <extension
         point="org.eclipse.ui.ide.markerResolution">
      <markerResolutionGenerator
            class="dk.itu.sdg.kopitiam.ResolutionGenerator"
            markerType="dk.itu.sdg.kopitiam.problemmarker">
      </markerResolutionGenerator>
   </extension>
   <extension
         point="org.eclipse.ui.navigator.navigatorContent">
      <commonFilter
            activeByDefault="true"
            class="dk.itu.sdg.kopitiam.NoOutputFoldersFilter"
            description="Hides Coq output folders."
            id="dk.itu.sdg.kopitiam.commonFilter.OutputFolderFilter"
            name="Coq output folders"
            visibleInUI="true">
      </commonFilter>
   </extension>
   <extension
         point="org.eclipse.ui.navigator.viewer">
      <viewerContentBinding
            viewerId="org.eclipse.ui.navigator.ProjectExplorer">
         <includes>
            <contentExtension
                  pattern="dk.itu.sdg.kopitiam.commonFilter.*">
            </contentExtension>
         </includes>
      </viewerContentBinding>
   </extension>

</plugin>
